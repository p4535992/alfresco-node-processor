{
  "collector": {
    "name": "QueryNodeCollector",
    "query": "TYPE:'cm:folder' AND PATH:'/app:company_home/*'"
  },
  "processor": {
    "name": "SetPermissionsProcessor",
    "permissions": {
      "isInheritanceEnabled": false,
      "locallySet": [
        {
          "authorityId": "GROUP_EVERYONE",
          "name": "Collaborator",
          "accessStatus": "ALLOWED"
        }
      ]
    },
    "readOnly": true
  }
}